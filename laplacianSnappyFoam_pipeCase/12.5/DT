FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "12.5";
    object      DT;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];


internalField   uniform 4e-05;

boundaryField
{
    topPatch
    {
        type            calculated;
        value           uniform 4e-05;
    }
    wall
    {
        type            calculated;
        value           uniform 4e-05;
    }
    bottomPatch
    {
        type            calculated;
        value           uniform 4e-05;
    }
}


